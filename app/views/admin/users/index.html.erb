<% if @users.empty? %>
    <p>There is no user</p>
<% else %>
  <table>
    <thead>
      <td>#</td>
      <td>Name</td>
      <td>Email</td>
      <td>tools</td>
    </thead>
    <% @users.each_with_index do |user, index| %>
      <tr>
      <td><%= index + 1 %></td>
      <td><%= link_to user.name, user_path(user)%></td>
      <td><%= user.email %></td>
      <td><%= link_to (I18n.t 'edit'), edit_admin_user_path(user) %> ||
        <%= link_to (I18n.t 'delete'), admin_user_path(user), method: 'delete', data: {confirm: (I18n.t 'confirm_delete')}%></td>
      </tr>
    <% end %>
  </table>
<% end %>